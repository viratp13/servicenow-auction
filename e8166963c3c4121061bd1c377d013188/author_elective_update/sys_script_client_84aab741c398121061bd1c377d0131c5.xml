    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Prevents submission if starting_price is 0 or less then 0</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Validate Starting Price</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {

    if (isLoading) {
        return;
    }

    if (newValue < 0) {
        g_form.showFieldMsg('starting_price', 'Starting Price cannot be negative. Please enter a non-negative value.', 'error');
    } else {
        g_form.clearMessages();
        g_form.hideFieldMsg('starting_price');
    }
}]]></script>
