        <messages/>
        <name>default increment amount </name>
        <order/>
        <script><![CDATA[function onLoad(control, oldValue, newValue) {
    var startingPriceField = 'starting_price'; // Field name for starting price
    var incrementAmountField = 'increment_amount'; // Field name for increment amount

    // Ensure the starting price field is available
    if (g_form.getControl(startingPriceField)) {
        // Fetch the value of the starting price field
        var startingPrice = parseFloat(g_form.getValue(startingPriceField));
        
        // Calculate 10% of the starting price
        var incrementAmount = startingPrice * 0.10;

        // Set the value for the increment amount field
        g_form.setValue(incrementAmountField, incrementAmount);
    } else {
        console.log(startingPriceField + ' field control is not available.');
    }
}
]]></script>
